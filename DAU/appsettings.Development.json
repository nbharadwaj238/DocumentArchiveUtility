{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MyConfig": {
    "StorageConnection": "",
    "Container": "ContainerName"
  },

  "Directory": {
    "Path": "Z:\\Write\\your\\folder\\name\\here\\"
  },

  "OtherDirectory": {
    "Path": "Z:\\Write\\your\\other\\foldername\\here\\"
  },

  "LogFile": {
    "Path": "C:\\inetpub\\logs\\LogFiles\\logs.txt"
  },

  "Services": {
    "Compressed": {
      "OlderThan": "-90" // Compressed on -60th day
    },
    "Archived": {
      "OlderThan": "-1" // Archieved on 2nd day of zip creation.
      //Note : Archieved date should not be same as compressed date to avoid deadlock situation.
    }
  },

  "ScheduleJob": {
    "MyCronJob1": {
      "CronExpression": "1 30 * * *"
    },
    "MyCronJob2": {
      "CronExpression": "5 30 * * *"
    },
    "MyCronJob3": {
      "CronExpression": "*/5 * * * *"
    }
  },

  "KeyVault": {
    "VaultName": "",
    "ClientId": "",
    "ClientSecret": "",
    "TenantId": "",
    "KvURL": "",
    "KeyName": "",
    "KeyVersion": ""
  },

  "AllowedHosts": "*"
}
