{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MyConfig": {
    "StorageConnection": "",
    "Container": "ContainerName"
  },

  "Directory": {
    "Path": "E:\\Write\\your\\FolderName\\"
  },

  "OtherDirectory": {
    "Path": "E:\\Write\\your\\OtherFolderName\\"
  },

  "LogFile": {
    "Path": "C:\\inetpub\\logs\\LogFiles\\DAU\\logs.txt"
  },

  "Services": {
    "Compressed": {
      "OlderThan": "-60" // Compressed on -60th day
    },
    "Archived": {
      "OlderThan": "-1" // Archieved on 2nd day of zip creation.
      //Note : Archieved date should not be same as compressed date to avoid deadlock situation.
    }
  },

  "ScheduleJob": {
    "MyCronJob1": {
      "CronExpression": "0 2 * * *"
    },
    "MyCronJob2": {
      "CronExpression": "0 5 * * *"
    },
    "MyCronJob3": {
      "CronExpression": "*/5 * * * *"
    }
  },

  "KeyVault": {
    "VaultName": "",
    "KvURL": "",
    "KeyName": "",
    "KeyVersion": ""
  },

  "AllowedHosts": "*"
}
